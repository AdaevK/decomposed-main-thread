{
  "name": "holyjs18-process-pool",
  "version": "1.0.0",
  "description": "HolyJS conf 2018 demos",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",

    "server": "node index.js",

    "start": "cross-env NODE_ENV=development babel-node src/index.js",
    "watch": "cross-env NODE_ENV=development babel-watch src/index.js",
    "build": "npm run build:client && npm run build:server && npm run build:node",
    "build:client": "cross-env NODE_ENV=production webpack -p --config webpack/client.prod.js",
    "build:server": "cross-env NODE_ENV=production webpack -p --config webpack/server.prod.js",
    "build:node": "cross-env NODE_ENV=production babel src/index.js -o public/index.js",

    "server:express": "npm run server -- --c 100",
    "server:fastify": "npm run server -- --c 101",
    "server:napa": "npm run server -- --c 102",
    "server:case1": "npm run server -- --c 1",
    "server:case2": "npm run server -- --c 2",
    "server:case3": "npm run server -- --c 3 | pino-elasticsearch",
    "server:case4": "npm run server -- --c 4 | pino-elasticsearch",
    "cluster:case1": "npm run server -- --c 5",
    "cluster:case2": "npm run server -- --c 6 | pino-elasticsearch",
    "cluster:case3": "npm run server -- --c 7 | pino-elasticsearch",
    "cluster:case4": "npm run server -- --c 8 | pino-elasticsearch",
    "cluster:case5": "npm run server -- --c 9 | pino-elasticsearch",
    "cluster:case6": "npm run server -- --c 10",
    "cluster:napa": "npm run server -- --c 11",
    "start:case1": "concurrently --kill-others-on-fail \"elasticsearch\" \"npm run server:case1\"",
    "start:case2": "concurrently --kill-others-on-fail \"elasticsearch\" \"npm run server:case2\"",
    "start:case3": "concurrently --kill-others-on-fail \"elasticsearch\" \"npm run server:case3\"",
    "start:case4": "concurrently --kill-others-on-fail \"elasticsearch\" \"npm run server:case4\"",
    "start:case5": "concurrently --kill-others-on-fail \"elasticsearch\" \"npm run server:case5\"",
    "start:case6": "concurrently --kill-others-on-fail \"elasticsearch\" \"npm run server:case6\"",
    "start:case7": "concurrently --kill-others-on-fail \"elasticsearch\" \"npm run server:case7\"",
    "start:case8": "concurrently --kill-others-on-fail \"elasticsearch\" \"npm run server:case8\"",
    "start:case9": "concurrently --kill-others-on-fail \"elasticsearch\" \"npm run server:case9\""
  },
  "keywords": [
    "main",
    "thread"
  ],
  "author": "Matvienko Nikolay",
  "license": "MIT",
  "dependencies": {
    "express": "^4.16.3",
    "fastify": "^1.3.1",
    "graphite": "^0.1.2",
    "measured": "^1.1.0",
    "napa": "^3.0.0",
    "napajs": "^0.2.3",
    "newrelic": "^4.1.0",
    "pino": "^4.16.1",
    "pino-elasticsearch": "^2.2.1",
    "pino-tee": "^0.2.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "winston": "^3.0.0-rc5",
    "winston-elasticsearch": "^0.5.9",
    "ejs": "^2.5.7",
    "react-universal-component": "^2.5.5",
    "webpack-flush-chunks": "^1.1.23"
  },
  "devDependencies": {
    "blocked-at": "^1.1.2",
    "concurrently": "^3.5.1",
    "minimist": "^1.2.0",
    "pmx": "^1.6.4",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.5",
    "css-hot-loader": "^1.3.1",
    "css-loader": "^0.28.7",
    "extract-css-chunks-webpack-plugin": "^2.0.17",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-loader": "^0.5.1",
    "serviceworker-webpack-plugin": "^0.2.3",
    "stats-webpack-plugin": "^0.6.1",
    "style-loader": "^0.18.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "webpack": "^3.6.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.19.1",
    "webpack-hot-server-middleware": "^0.1.0",
    "webpack-merge": "^4.1.0"
  }
}
